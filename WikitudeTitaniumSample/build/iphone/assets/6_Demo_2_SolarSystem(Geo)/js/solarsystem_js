var Solar={planetsInfo:null,init:function(){var e=580.2,t=new AR.RelativeLocation(null,25e3,0,5e3),i=.5,n=320,r=new AR.ImageResource("assets/Sun-icon.png"),a=new AR.ImageResource("assets/Mercury-icon.png"),o=new AR.ImageResource("assets/Venus-icon.png"),s=new AR.ImageResource("assets/Earth-icon.png"),l=new AR.ImageResource("assets/Mars-icon.png"),c=new AR.ImageResource("assets/Jupiter-icon.png"),d=new AR.ImageResource("assets/Saturn-icon.png"),u=new AR.ImageResource("assets/Uranus-icon.png"),f=new AR.ImageResource("assets/Neptune-icon.png"),h=new AR.ImageResource("assets/Pluto-icon.png"),_=new AR.ImageResource("assets/indi.png"),A=.3*(109*n/n)*i,R=4.9*n/n*i,g=12*n/n*i,p=10*n/n*i,E=6.8*n/n*i,T=14.4*n/n*i,O=12*n/n*i,m=5.2*n/n*i,b=5*n/n*i,v=.23*n/n*i,I={name:"Sun",distance:0,location:t,imgDrawable:new AR.ImageDrawable(r,A),size:A,description:"The Sun is the star at the center of the Solar System. It is almost perfectly spherical and consists of hot plasma interwoven with magnetic fields.",mass:"2&nbsp;10<sup>30</sup>&nbsp;kg",diameter:"1,392,684&nbsp;km"},y={name:"Mercury",distance:3.9*e,location:new AR.RelativeLocation(t,0,-3.9*e,0),imgDrawable:new AR.ImageDrawable(a,R),size:R,description:"Is the innermost planet in the Solar System. It is also the smallest, and its orbit is the most eccentric (that is, the least perfectly circular).",mass:"3.3&nbsp;10<sup>23</sup>&nbsp;kg",diameter:"4,880&nbsp;km"},D={name:"Venus",distance:7.2*e,location:new AR.RelativeLocation(t,0,-7.2*e,0),imgDrawable:new AR.ImageDrawable(o,g),size:g,description:"Is named after the Roman goddess of love and beauty. After the Moon, it is the brightest natural object in the night sky, bright enough to cast shadows.",mass:"4.9&nbsp;10<sup>24</sup>",diameter:"12,092&nbsp;km"},C={name:"Earth",distance:10*e,location:new AR.RelativeLocation(t,0,-10*e,0),imgDrawable:new AR.ImageDrawable(s,p),size:p,description:"Is the third planet from the Sun, and the densest and fifth-largest of the eight planets in the Solar System. It is sometimes referred to as the world, the Blue Planet, Terra.",mass:" 6&nbsp;10<sup>24</sup>&nbsp;kg",diameter:"12,742&nbsp;km"},P={name:"Mars",distance:15*e,location:new AR.RelativeLocation(t,0,-15*e,0),imgDrawable:new AR.ImageDrawable(l,E),size:E,description:"Is named after the Roman god of war, it is often described as the &quot;Red Planet&quot;, as the iron oxide prevalent on its surface gives it a reddish appearance.",mass:"6.4&nbsp;10<sup>23</sup>&nbsp;kg",diameter:"6794&nbsp;km"},L={name:"Jupiter",distance:.35*50.2*e,location:new AR.RelativeLocation(t,0,.35*-50.2*e,0),imgDrawable:new AR.ImageDrawable(c,T),size:T,description:"Is the largest planet within the Solar System. It is a gas giant with mass one-thousandth that of the Sun but is two and a half times the mass of all the other planets in our Solar System combined.",mass:"1.9&nbsp;10<sup>27</sup>&nbsp;kg",diameter:"142,984&nbsp;km"},N={name:"Saturn",distance:.2*95.3*e,location:new AR.RelativeLocation(t,0,.2*-95.3*e,0),imgDrawable:new AR.ImageDrawable(d,O),size:O,description:"Is the second largest planet in the Solar System, after Jupiter. Named after the Roman god Saturn. Saturn is a gas giant with an average radius about nine times that of Earth.",mass:"5.7&nbsp;10<sup>26</sup>&nbsp;kg",diameter:"25,000&nbsp;km"},S={name:"Uranus",distance:.1*192*e,location:new AR.RelativeLocation(t,0,.1*-192*e,0),imgDrawable:new AR.ImageDrawable(u,m),size:m,description:"It has the third-largest planetary radius and fourth-largest planetary mass in the Solar System. It is named after the ancient Greek deity of the sky Uranus.",mass:"8.7&nbsp;10<sup>25</sup>&nbsp;kg",diameter:"51,118&nbsp;km"},w={name:"Neptune",distance:.07*301*e,location:new AR.RelativeLocation(t,0,.07*-301*e,0),imgDrawable:new AR.ImageDrawable(f,b),size:b,description:"Is the farthest planet from the Sun in the Solar System. It is the fourth-largest planet by diameter and the third-largest by mass.",mass:"1&nbsp;10 <sup>26</sup>&nbsp;kg",diameter:"49,532&nbsp;km"},Y={name:"Pluto",distance:.063*394*e,location:new AR.RelativeLocation(t,0,.063*-394*e,0),imgDrawable:new AR.ImageDrawable(h,v),size:v,description:"Originally classified as the ninth planet from the Sun, Pluto was recategorized as a dwarf planet and plutoid owing to the discovery that it is only one of several large bodies within the Kuiper belt.",mass:"1.3&nbsp;10<sup>22</sup>&nbsp;kg",diameter:"2,306&nbsp;km"};this.planetsInfo=[I,y,D,C,P,L,N,S,w,Y];for(var j=[],k=0;this.planetsInfo.length>k;k++){var x=new AR.Label(this.planetsInfo[k].name,3,{offsetY:-this.planetsInfo[k].size/2,verticalAnchor:AR.CONST.VERTICAL_ANCHOR.TOP,opacity:.9,zOrder:1,style:{textColor:"#FFFFFF",backgroundColor:"#00000005"}}),V=[];if(V[0]=this.planetsInfo[k].imgDrawable,V[1]=x,j[k]=new AR.GeoObject(this.planetsInfo[k].location,{drawables:{cam:V},onClick:this.planetClicked(this.planetsInfo[k])}),k>0)this.animate(this.planetsInfo[k]);else{var B=new AR.PropertyAnimation(this.planetsInfo[k].location,"northing",1e4,1e4,1e3,{type:AR.CONST.EASING_CURVE_TYPE.EASE_IN_SINE});B.start(-1)}}var U=new AR.ImageDrawable(_,.1,{verticalAnchor:AR.CONST.VERTICAL_ANCHOR.TOP});j[0].drawables.addIndicatorDrawable(U)},animate:function(e){var t=e.location,i=e.distance,n=2*2*i,r=new AR.PropertyAnimation(t,"northing",1*i,0*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_IN_SINE}),a=new AR.PropertyAnimation(t,"easting",0*i,1*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_OUT_SINE}),o=new AR.PropertyAnimation(t,"northing",0*i,-1*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_OUT_SINE}),s=new AR.PropertyAnimation(t,"easting",1*i,0*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_IN_SINE}),l=new AR.PropertyAnimation(t,"northing",-1*i,0*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_IN_SINE}),c=new AR.PropertyAnimation(t,"easting",0*i,-1*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_OUT_SINE}),d=new AR.PropertyAnimation(t,"northing",0*i,1*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_OUT_SINE}),u=new AR.PropertyAnimation(t,"easting",-1*i,0*i,n/4,{type:AR.CONST.EASING_CURVE_TYPE.EASE_IN_SINE}),f=new AR.AnimationGroup(AR.CONST.ANIMATION_GROUP_TYPE.PARALLEL,[r,a]),h=new AR.AnimationGroup(AR.CONST.ANIMATION_GROUP_TYPE.PARALLEL,[o,s]),_=new AR.AnimationGroup(AR.CONST.ANIMATION_GROUP_TYPE.PARALLEL,[l,c]),A=new AR.AnimationGroup(AR.CONST.ANIMATION_GROUP_TYPE.PARALLEL,[d,u]),R=new AR.AnimationGroup(AR.CONST.ANIMATION_GROUP_TYPE.SEQUENTIAL,[A,f,h,_]);R.start(-1)},planetClicked:function(e){return function(){document.getElementById("info").setAttribute("class","info"),document.getElementById("name").innerHTML=e.name,document.getElementById("mass").innerHTML=e.mass,document.getElementById("diameter").innerHTML=e.diameter,document.getElementById("info").setAttribute("class","infoVisible")}}};Solar.init();