function ArWindow(e,t){var i=require("jsuri-1.1.1"),n=require("util"),r=require("com.wikitude.ti"),a=Ti.UI.createWindow({backgroundColor:"transparent",navBarHidden:!0,title:"ArWindow"}),o=Ti.UI.createView({backgroundColor:"#f2f2f2",left:0,right:0,top:0,height:48});a.add(o);var s=Ti.UI.createLabel({text:"AR"});o.add(s);var l=Ti.UI.createButton({title:"Back",left:6,top:6,height:36,width:64});o.add(l);var c=Ti.UI.createView({backgroundColor:"#ffffff",bottom:0,left:0,right:0,top:48});a.add(c);var d=null,u=function(e){new i.Uri(e.url)},f=null;if(n.isAndroid())f=t;else{var h=Ti.Filesystem.getFile(Ti.Filesystem.resourcesDirectory,t);f=h.nativePath}return a.addEventListener("open",function(){d=r.createWikitudeView({licenseKey:"foo",bottom:0,left:0,right:0,top:0}),c.add(d),d.addEventListener("URL_WAS_INVOKED",u),setTimeout(function(){f&&(d.architectWorldUri=f)},2e3)}),a.addEventListener("close",function(){null!=d&&(d.removeEventListener("URL_WAS_INVOKED",u),c.remove(d),d=null)}),n.isAndroid()&&a.addEventListener("android:back",function(){a.close()}),l.addEventListener("click",function(){a.close()}),a}module.exports=ArWindow;