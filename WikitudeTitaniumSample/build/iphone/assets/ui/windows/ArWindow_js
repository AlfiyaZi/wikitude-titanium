function ArWindow(e,r){var t=require("jsuri-1.1.1"),n=require("util"),o=require("com.wikitude.ti"),i=Ti.UI.createWindow({backgroundColor:"transparent",navBarHidden:!0,title:"ArWindow"}),u=Ti.UI.createView({backgroundColor:"#f2f2f2",left:0,right:0,top:0,height:48});i.add(u);var a=Ti.UI.createLabel({text:"AR"});u.add(a);var s=Ti.UI.createButton({title:"Back",left:6,top:6,height:36,width:64});u.add(s);var c=Ti.UI.createView({backgroundColor:"#ffffff",bottom:0,left:0,right:0,top:48});i.add(c);var f=null,l=function(e){new t.Uri(e.url)},d=null;if(n.isAndroid())d=r;else{var h=Ti.Filesystem.getFile(Ti.Filesystem.resourcesDirectory,r);d=h.nativePath}return i.addEventListener("open",function(){f=o.createWikitudeView({licenseKey:"foo",bottom:0,left:0,right:0,top:0}),c.add(f),f.addEventListener("URL_WAS_INVOKED",l),setTimeout(function(){d&&(f.architectWorldUri=d)},2e3)}),i.addEventListener("close",function(){null!=f&&(f.removeEventListener("URL_WAS_INVOKED",l),c.remove(f),f=null)}),n.isAndroid()&&i.addEventListener("android:back",function(){i.close()}),s.addEventListener("click",function(){i.close()}),i}module.exports=ArWindow;