function MainWindow(){var e=require("util"),t=Ti.UI.createWindow({backgroundColor:"transparent",navBarHidden:!0,title:"MainWindow",exitOnClose:!0}),r=Ti.UI.createView({backgroundColor:"#f2f2f2",left:0,right:0,top:0,height:48});t.add(r);var n=Ti.UI.createLabel({text:"MAIN"});r.add(n);var i=Ti.UI.createView({backgroundColor:"#ffffff",bottom:0,left:0,right:0,top:48});t.add(i);var o=[],a=[{title:"AR sample world",callback:function(){var e=require("/ui/windows/ArWindow");new e("foo","http://goo.gl/Vs8Oc",o).open()}}],u=Ti.UI.createTableView({data:a});return i.add(u),u.addEventListener("click",function(e){a[e.index].callback()}),e.isAndroid()&&t.addEventListener("android:back",function(){t.close()}),t.addEventListener("close",function(){e.isAndroid()&&Ti.Android.currentActivity.finish()}),t}module.exports=MainWindow;