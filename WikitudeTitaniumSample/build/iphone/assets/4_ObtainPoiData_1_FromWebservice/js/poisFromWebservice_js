var ServerInformation={POIDATA_SERVER:"http://example.wikitude.com/GetSamplePois/",POIDATA_SERVER_ARG_LAT:"lat",POIDATA_SERVER_ARG_LON:"lon",POIDATA_SERVER_ARG_NR_POIS:"nrPois"},World={markerDrawable_idle:null,markerDrawable_selected:null,markerDrawable_directionIndicator:null,markerList:[],loadPoisFromJsonData:function(e){AR.context.destroyAll(),World.markerDrawable_idle=new AR.ImageResource("assets/marker_idle.png"),World.markerDrawable_selected=new AR.ImageResource("assets/marker_selected.png"),World.markerDrawable_directionIndicator=new AR.ImageResource("assets/indi.png"),document.getElementById("statusElement").innerHTML="Loading JSON objects";for(var t=0;e.length>t;t++){var i={latitude:parseFloat(e[t].latitude),longitude:parseFloat(e[t].longitude),altitude:parseFloat(e[t].altitude),title:e[t].name,description:e[t].description};World.markerList.push(new Marker(i))}document.getElementById("statusElement").innerHTML="JSON objects loaded properly"},userLocation:null,alreadyRequestedData:!1,locationChanged:function(e,t,i,n){World.userLocation={latitude:e,longitude:t,altitude:i,accuracy:n},World.alreadyRequestedData||(World.requestDataFromServer(e,t),World.alreadyRequestedData=!0)},requestDataFromServer:function(e,t){var i=ServerInformation.POIDATA_SERVER+"?"+ServerInformation.POIDATA_SERVER_ARG_LAT+"="+e+"&"+ServerInformation.POIDATA_SERVER_ARG_LON+"="+t+"&"+ServerInformation.POIDATA_SERVER_ARG_NR_POIS+"=20";$.getJSON(i,function(e){World.loadPoisFromJsonData(e)}).error(function(e){alert("JSON error occured! "+e.message)}).complete(function(){})}};AR.context.onLocationChanged=World.locationChanged;