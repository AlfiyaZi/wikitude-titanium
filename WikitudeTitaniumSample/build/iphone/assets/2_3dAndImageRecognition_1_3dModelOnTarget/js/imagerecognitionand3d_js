var World={loaded:!1,rotating:!1,init:function(){this.createOverlays()},createOverlays:function(){this.tracker=new AR.Tracker("assets/tracker.wtc",{onLoaded:this.loadingStep}),this.modelCar=new AR.Model("assets/car.wt3",{onLoaded:this.loadingStep,scale:{x:.045,y:.045,z:.045},translate:{x:0,y:.05,z:0},rotate:{roll:-25}}),new AR.Trackable2DObject(this.tracker,"carAd",{drawables:{cam:[this.modelCar]},onEnterFieldOfVision:this.appear})},loadingStep:function(){!World.loaded&&World.tracker.isLoaded()&&World.modelCar.isLoaded()&&(World.loaded=!0,document.body.removeChild(document.getElementById("loadingMessage")))}};World.init();